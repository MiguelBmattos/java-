import java.util.Scanner;
  public class Main {
   public static void main(String[] args) {
    Scanner scanner=new Scanner(System.in);
    System.out.print("digite um numero:");
    int num=scanner.nextInt();
    long f=num;
    
      while(num>1){
        f=f*(num-1);
        num--;
        
        System.out.println(f);
        
      }
     
      }
    }
